#include <iostream>
using namespace std;

int main() {
    double num1, num2;
    char op;

    cin >> num1;
    cin >> op;
    cin >> num2;

    switch(op) {
        case '+':
            cout << num1 + num2;
            break;
        case '-':
            cout << num1 - num2;
            break;
        case '*':
            cout << num1 * num2;
            break;
        case '/':
            if(num2 != 0)
                cout << num1 / num2;
            else
                cout << "Error";
            break;
        default:
            cout << "Invalid";
    }
    return 0;
}
